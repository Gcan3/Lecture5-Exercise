#include <iostream>
#include <string>

using namespace std;
int main() 
{
	double farenheit;

	cout << "Enter Farenheit temperature:" << endl;
	cin >> farenheit;

	double celsius = ((farenheit - 32) * (0.556));

	cout << farenheit << " degrees Farenheit is " << celsius << " degrees in Celsius" << endl;

	return 0;
}

#include <iostream>
#include <string>

using namespace std;
int main() 
{
	string name;
	string home;
	int age;

	cout << "Enter first name: ";
	cin >> name;

	cout << "Enter home: ";
	cin >> home;

	cout << "Enter age: ";
	cin >> age;

	cout << "Name: " << name << endl;
	cout << "Home: " << home << endl;
	cout << "Age: " << age;

	return 0;
}
